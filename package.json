{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Host Next.js SSR app on Firebase Cloud Functions with Firebase Hosting redirects. Built with typescript.",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "dev": "yarn clean && next src/app",
    "preserve": "yarn clean && yarn build-public && yarn build-functions && yarn build-app && yarn copy-deps && yarn install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "firebase deploy",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build-app": "next build \"src/app\"",
    "build-public": "cpx \"src/public/**/*.*\" \"dist/public\" -C && cpx \"src/app/public/**/*.*\" \"dist/public\"",
    "build-functions": "tsc --project src/functions",
    "lint-app": "tslint --project src/app",
    "typecheck-app": "tsc --project src/app",
    "lint-functions": "tslint --project src/functions",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && yarn"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "firebase-admin": "^8.0.0",
    "firebase-functions": "^3.1.0",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.0",
    "gray-matter": "^4.0.2",
    "isomorphic-unfetch": "3.0.0",
    "next": "latest",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.2.0",
    "node-sass": "^4.13.0",
    "raw-loader": "^4.0.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-markdown": "^4.2.2",
    "react-svg-loader": "^3.0.3",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "cpx": "1.5.0",
    "cross-env": "5.2.0",
    "firebase-tools": "^7.1.0",
    "rimraf": "2.6.2",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "@types/node": "^12.7.8",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "typescript": "3.6.3",
    "@types/webpack-env": "^1.14.1"
  },
  "license": "ISC"
}
