{
  "hosting": {
    "public": "dist",
    "predeploy": [
      "yarn clean",
      "yarn typecheck-app",
      "yarn build-app",
      "yarn export"
    ],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/works",
        "run": {
          "serviceId": "portfolio",
          "region": "asia-east1"
        }
      }
    ]
  }
}
